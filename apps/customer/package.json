{
  "private": true,
  "name": "customer-app",
  "version": "1.0.0",
  "repository": "https://github.com/Helpdor/frontend/app-hero",
  "license": "private",
  "engines": {
    "node": "^18.18",
    "npm": "^9.8"
  },
  "main": "index.ts",
  "scripts": {
    "build:eas:android:development": "eas build --profile development --platform android --clear-cache",
    "build:eas:android:production": "eas build --profile production --platform android --clear-cache",
    "build:eas:ios:development": "eas build --profile development --platform ios --clear-cache",
    "build:eas:ios:production": "eas build --profile production --platform ios --clear-cache",
    "update:eas:android:production": "eas update --branch production --platform android --clear-cache",
    "update:eas:ios:production": "eas update --branch production --platform ios --clear-cache",
    "submit:eas:android": "eas submit --platform android",
    "submit:eas:ios": "eas submit --platform ios",
    "dev": "expo start",
    "start": "expo start --go",
    "start:android": "npm run start -- --android",
    "start:ios": "npm run start -- --ios",
    "start:web": "npm run start -- --web",
    "pretty": "prettier --write . --log-level warn",
    "lint": "eslint . --fix --rule \"react-hooks/exhaustive-deps: off\"",
    "reinstall:dependencies": "rm -rf node_modules && npm i",
    "reinstall:dependencies:full": "npm cache clean --force && npm run reinstall:dependencies"
  },
  "dependencies": {
    "@hero24/common": "*",
    "@hero24/ui-library": "*",
    "@apollo/client": "^3.8.3",
    "@expo/vector-icons": "^13.0.0",
    "@gluestack-style/react": "^0.2.46",
    "@gluestack-ui/themed": "^0.1.16",
    "@react-navigation/native": "^6.0.2",
    "expo": "~49.0.11",
    "expo-application": "~5.3.0",
    "expo-constants": "^14.4.2",
    "expo-device": "~5.4.0",
    "expo-font": "~11.4.0",
    "expo-linking": "~5.0.2",
    "expo-router": "^2.0.0",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "~2.4.0",
    "expo-updates": "^0.18.13",
    "expo-web-browser": "~12.3.2",
    "react": "18.2.0",
    "react-combine-providers": "^0.9.6",
    "react-dom": "18.2.0",
    "react-native": "0.72.5",
    "react-native-dotenv": "^3.4.9",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-web": "~0.19.6",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.7",
    "@cspell/eslint-plugin": "^7.3.6",
    "@types/fs-extra": "^11.0.2",
    "@types/i18n-js": "^3.8.5",
    "@types/lodash": "^4.14.195",
    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-typescript-sort-keys": "^3.0.0",
    "fs-extra": "^11.1.1",
    "prettier": "^3.0.3",
    "semver": "^7.5.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "overrides": {
    "react-refresh": "~0.14.0"
  },
  "resolutions": {
    "react-refresh": "~0.14.0"
  }
}
