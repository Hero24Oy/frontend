{
  "private": true,
  "name": "hero-app",
  "version": "1.0.3",
  "repository": "https://github.com/Helpdor/frontend/apps/hero",
  "license": "private",
  "engines": {
    "node": "^18.18"
  },
  "main": "index.ts",
  "scripts": {
    "prebuild": "npx expo prebuild",
    "build:eas:android:development": "yarn prebuild && eas build --profile development --platform android --clear-cache",
    "build:eas:android:production": "yarn prebuild && eas build --profile production --platform android --clear-cache",
    "build:eas:ios:development": "yarn prebuild && eas build --profile development --platform ios --clear-cache",
    "build:eas:ios:production": "yarn prebuild && eas build --profile production --platform ios --clear-cache",
    "update:eas:android:production": "eas update --branch production --platform android --clear-cache",
    "update:eas:ios:production": "eas update --branch production --platform ios --clear-cache",
    "submit:eas:android": "eas submit --platform android",
    "submit:eas:ios": "eas submit --platform ios",
    "watch": "npx expo start --go -c",
    "pretty": "prettier --write . --log-level warn",
    "lint": "eslint . --fix",
    "reinstall:dependencies": "rm -rf node_modules && npm i",
    "reinstall:dependencies:full": "npm cache clean --force && yarn reinstall:dependencies",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
